{
  "myfox-wrapper-api": {
    "server": {
      "hapi": {
        "options": {},
        "main-connection": {
          "host": "localhost",
          "port": 3001
        }
      },
      "myfox": {
        "myfoxSiteIds": []
      }
    },
    "html": {
      "myfox": {
        "protocol": "https",
        "hostname": "myfox.me",
        "port": 443,
        "headers_login": {
          "Origin": "https://myfox.me"
        },
        "headers": {
          "Accept": "application/json, text/javascript, */*; q=0.01",
          "Accept-Encoding": "gzip, deflate",
          "Accept-Language": "en-US,en;q=0.8,fr;q=0.6",
          "Cache-Control": "no-cache",
          "Connection": "keep-alive",
          "DNT": "1",
          "Pragma": "no-cache",
          "User-Agent": "Mozilla/5.0 (Windows NT 6.1) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/48.0.2564.103 Safari/537.36",
          "X-Requested-With": "XMLHttpRequest",
          "X-SmartResponse": "1"
        },
        "redirectForbidden": "https://myfox.me/",
        "paths": {
          "login": "/login",
          "home": "/home/{siteId}",
          "scenario_action": "/widget/{siteId}/scenario/{action}/{id}",
          "domotic_action": "/widget/{siteId}/domotic/{action}/{id}",
          "heating_action": "/widget/{siteId}/heating/{action}/{id}",
          "alarm_action": "/widget/{siteId}/protection/seclev/{level}",
          "scenario_edition_temp_1": "/scenario/{siteId}/manage/{scenarioId}/1",
          "scenario_edition_temp_2": "/scenario/{siteId}/manage/{scenarioId}/2",
          "scenario_edition_temp_3": "/scenario/{siteId}/manage/{scenarioId}/3",
          "scenario_edition_temp_4": "/scenario/{siteId}/manage/{scenarioId}/4",
          "scenario_edition_temp_5": "/scenario/{siteId}/manage/{scenarioId}/5"
        }
      }
    }
  }
}
